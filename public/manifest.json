{
  "manifest_version": 3,
  "name": "UMN Classes To Calendar",
  "version": "1.0.0",
  "description": "Converts UMN class schedules on MyU to calendar file",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "action": {
    "default_title": "UMN Classes To Calendar"
  },
  "permissions": [
    "declarativeContent",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.myu.umn.edu/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}